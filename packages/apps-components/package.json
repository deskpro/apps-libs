  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --out-dir ./build --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./build/es --ignore *.test.js",
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:es && npm run build-storybook",
    "build-storybook": "build-storybook -c .storybook -o gen/storybook",
    "dev": "start-storybook -s storybook/static -p 6006"
  },
  "dependencies": {
    "@deskpro/apps-components-style": "^0.3.6",
    "classnames": "^2.2.6",
    "react-custom-scrollbars": "^4.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
