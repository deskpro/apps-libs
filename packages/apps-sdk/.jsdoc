{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "DeskPRO Apps SDK",
     "cleverLinks": true,
     "monospaceLinks": true,
      "default": {
        "includeDate": false
      }
  },
  "opts": {
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "node_modules/docdash"
  }
}
