{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "ie >= 11"
          ]
        },
        "exclude": [],
        "modules": false,
        "loose": true
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-es2015-modules-commonjs"
  ],
  "env": {
    "test": {
      "presets": [["env", {
        "modules": "commonjs",
        "loose": true
      }]],
      "plugins": [
        "transform-runtime",
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
