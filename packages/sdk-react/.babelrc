{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "ie >= 11"
          ]
        },
        "exclude": [],
        "modules": false,
        "loose": true
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties"
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["env", { "loose": true } ],
        "react"
      ]
    },
    "test": {
      "presets": [
        ["env", { "loose": true } ],
        "react"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "babel-plugin-transform-class-properties"
      ]
    }
  }
}
