{
  "name": "@deskpro/apps-sdk-react",
  "version": "2.0.0-beta.1",
  "description": "An SDK for building DeskPRO Apps powered by the React library.",
  "main": "lib/sdk-react.js",
  "module": "es/sdk-react.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test build",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/sdk-react.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/sdk-react.js",
    "build": "npm run build:commonjs && npm run build:es",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test && npm run build",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "@deskpro/apps-sdk-core": "^2.0.0",
    "@deskpro/apps-components": "^0.3.6",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.1",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "documentation": "^5.3.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "jsdoc-babel": "^0.3.0",
    "react-svg-inline": "^2.1.1",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@deskpro/apps-sdk-core": "^2.0.0",
    "@deskpro/apps-components": "^0.3.6",
    "raf": "^3.4.0",
    "prop-types": "^15.6.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "jest": {
    "testRegex": "(/test/.*\\.test.js)$"
  },
  "optionalDependencies": {
    "changelog-verify": "1.1.0",
    "version-changelog": "3.1.0"
  }
}
