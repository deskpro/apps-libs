{
  "name": "@deskpro/apps-sdk-react",
  "version": "3.0.0",
  "description": "An SDK for building DeskPRO Apps powered by the React library.",
  "main": "lib/sdk-react.js",
  "module": "es/sdk-react.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test ",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "dev": "cross-env NODE_ENV=development rollup --watch -c -o lib/sdk-react.js",
    "build": "cross-env NODE_ENV=production rollup -c -o lib/sdk-react.js",
    "prepare": "npm run clean && npm run build"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "@deskpro/apps-sdk-core": "^3.0.0",
    "@deskpro/apps-components": "^0.3.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "documentation": "^8.0.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^5.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.1",
    "jest": "^23.4.1",
    "jsdoc-babel": "^0.4.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "@deskpro/apps-components": "^0.3.6",
    "@deskpro/apps-sdk-core": "^3.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "jest": {
    "testRegex": "(/test/.*\\.test.js)$"
  }
}
